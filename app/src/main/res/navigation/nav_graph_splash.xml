<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_splash"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="network.etna.etnawallet.MainFragment">

        <action
            android:id="@+id/profileFragment_to_importWalletActivity"
            app:destination="@id/importWalletActivity"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/fade_out"
            />

    </fragment>

    <activity
        android:id="@+id/importWalletActivity"
        android:name="network.etna.etnawallet.ui.addwallet.ImportWalletActivity"/>

</navigation>